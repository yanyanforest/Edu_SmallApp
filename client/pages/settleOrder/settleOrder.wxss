.course-footer {
  position: absolute;
  bottom: 0px;
  width: 100%;
  height: 44px;
  margin-bottom: 0px;
  display: flex;
	flex:1;
  margin: 0;
  border-top: lightgray solid 1rpx;
  justify-content: center;
	flex-direction: row;
}

.course-footer .price {
  text-align: center;
  line-height: 44px;
  background: #f7f7f7;
	position: relative;
flex: 70%;
}

.course-footer .pay {
  flex: 130%;
  line-height: 44px;
  position: relative;
  background-color: #ff6700;
  color: #f7f7f7;
  font-size: 34rpx;
  text-align: center;
}
