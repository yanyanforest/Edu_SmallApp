/* pages/payOrder/payOrder.wxss */
 @import "../commons/price.wxss"; 

.container{
	flex-direction: column;
	width: 100%;
	
}
.header{
background: #ffffff;
width: 100%;
flex: 1;

}
.header-tips{
background: #fdece9;
width: 100%;
flex: 1;
height: 45px;
text-align: center;
font-size: 30rpx;
color: #ef4023;
line-height: 44px;
}
.header-bg{
		position: relative;
	background:url(data:image/png;base64,) no-repeat;
	 align-content: center;
	 align-items: center;
margin-bottom: 0px;
  display: flex;
	  justify-content:center; 
	flex-direction: column;
}
 .header-content{
	 position: absolute;
	 width: 70%;
	 align-content: center;
	 align-items: center;
  display: flex;
	justify-content:center; 
	flex-direction: column;
	bottom: 20px;

}
.header-content .order-price{
	
text-align: center;
color: #333333;
font-size: 18px;

}
.header-content .order-sn{
	margin-top: 10rpx;
color: #808080;
font-size: 14px;
text-align: center;
}
.header-pay
{
	width: 100%;
	line-height: 44px;
	background: #f5f5f5;
	font-size: 28rpx;
	color: #808080;
	padding-left: 30rpx;
	text-align: left;
}
.weui-cells{
	margin-top: 0;
	border-top: none;
	width: 100%;
}
.weui-cell__bd{
	width: 100%;
	flex-direction: column;
	justify-content: center;
}
