/* pages/settleOrder/settleOrder.wxss */
@import "../commons/price.wxss";

.container {
  display: flex;
  flex-direction: column;
  width: 100%;
  margin: 0;
}

.header {
  margin: 0px;
  top: 0px;
  background: url(data:image/png;base64,) no-repeat;
}

/* 关于课程的  */

.course-content {
  margin: 10px 10px 5px;
  border-bottom: #c1c1c1 solid 0.5px;
  border-radius: 3px;
  background: white;
  height: 110px;
}

.course-content image {
  border-radius: 5px;
  float: left;
  padding: 20px 10px;
  width: 120px;
  height: 70px;
}
.course-content view {
 display: block; 
  margin: 5rpx 0;
}  
.course-content .title {
  font-size: 30rpx;
  padding-top: 20px;
  line-height: 40px;
  padding-left: 5rpx;
  margin: 5rpx 0;
  text-align: left;
  display: block;
}

